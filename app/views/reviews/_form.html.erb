<%= form_for [ @movie, @review ] do |f| %>
   
  <div class="field">
    <div id="star_rating"></div>
  </div>
  <div class="field">
    <%= f.label :comment %><br>
    <%= f.text_area :comment %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<script>
  $('#star_rating').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
  });
</script>